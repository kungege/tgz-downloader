{
  "name": "tgz-downloader",
  "type": "module",
  "version": "2.0.4",
  "description": "download tgz package",
  "author": "kalykun <984757534@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/kungege/tgz-downloader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kungege/tgz-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/kungege/tgz-downloader/issues"
  },
  "keywords": [
    "download",
    "tgz",
    "tgz-downloader",
    "nexus"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "tgz-download": "bin/download-tgz.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "esno src/index.ts",
    "build": "unbuild",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@yarnpkg/lockfile": "^1.1.0",
    "axios": "^1.13.4",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "consola": "^3.4.2",
    "minimist": "^1.2.8",
    "mkdirp": "^3.0.1",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@types/minimist": "^1.2.5",
    "@types/node": "^25.2.1",
    "bumpp": "^10.4.1",
    "eslint": "^9.39.2",
    "esno": "^4.8.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
