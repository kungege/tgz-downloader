{
  "name": "tgz-downloader",
  "type": "module",
  "version": "1.0.8",
  "packageManager": "pnpm@8.2.0",
  "description": "download tgz package",
  "author": "kalykun <984757534@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/kungege/tgz-downloader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kungege/tgz-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/kungege/tgz-downloader/issues"
  },
  "keywords": [
    "download",
    "tgz",
    "tgz-downloader",
    "nexus"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "tgz-download": "bin/download-tgz.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "esno src/index.ts",
    "build": "unbuild",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@yarnpkg/lockfile": "^1.1.0",
    "axios": "^1.3.6",
    "chalk": "^5.2.0",
    "commander": "^11.0.0",
    "consola": "^3.1.0",
    "minimist": "^1.2.8",
    "mkdirp": "^3.0.1",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.1.0",
    "bumpp": "^9.1.0",
    "eslint": "^8.40.0",
    "esno": "^0.16.3",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
